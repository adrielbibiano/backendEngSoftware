# Example environment variables for sistema-coleta-backend
# Copy this to .env and fill values before running in production

# Database connection (PostgreSQL)
# DATABASE_URL=postgresql://user:password@host:port/dbname

# JWT secret used to sign tokens
JWT_SECRET=replace_this_with_a_secure_secret

# Frontend URL allowed for CORS
FRONTEND_URL=http://localhost:5174

# Public API URL shown in Swagger
API_URL=http://localhost:3000
